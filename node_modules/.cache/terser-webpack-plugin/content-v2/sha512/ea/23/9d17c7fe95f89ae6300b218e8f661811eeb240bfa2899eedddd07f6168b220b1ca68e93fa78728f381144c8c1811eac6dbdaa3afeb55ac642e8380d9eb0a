ace Pirates\",difficulty:h,boss:!1},te={name:\"Ship Command\",difficulty:g,boss:!1},ne={name:\"Badoon Headhunter\",difficulty:g,boss:!1},re={name:\"Kree Fanatic\",difficulty:f+g,boss:!1},se=(n={},Object(d[\"a\"])(n,b.name,b),Object(d[\"a\"])(n,y.name,y),Object(d[\"a\"])(n,v.name,v),Object(d[\"a\"])(n,q.name,q),Object(d[\"a\"])(n,x.name,x),Object(d[\"a\"])(n,_.name,_),Object(d[\"a\"])(n,H.name,H),Object(d[\"a\"])(n,O.name,O),Object(d[\"a\"])(n,C.name,C),Object(d[\"a\"])(n,S.name,S),Object(d[\"a\"])(n,j.name,j),Object(d[\"a\"])(n,w.name,w),Object(d[\"a\"])(n,k.name,k),Object(d[\"a\"])(n,M.name,M),Object(d[\"a\"])(n,P.name,P),Object(d[\"a\"])(n,G.name,G),Object(d[\"a\"])(n,E.name,E),Object(d[\"a\"])(n,L.name,L),Object(d[\"a\"])(n,A.name,A),Object(d[\"a\"])(n,I.name,I),Object(d[\"a\"])(n,R.name,R),Object(d[\"a\"])(n,N.name,N),Object(d[\"a\"])(n,T.name,T),Object(d[\"a\"])(n,J.name,J),Object(d[\"a\"])(n,Y.name,Y),Object(d[\"a\"])(n,Q.name,Q),Object(d[\"a\"])(n,U.name,U),Object(d[\"a\"])(n,D.name,D),Object(d[\"a\"])(n,W.name,W),Object(d[\"a\"])(n,B.name,B),Object(d[\"a\"])(n,re.name,re),Object(d[\"a\"])(n,$.name,$),Object(d[\"a\"])(n,F.name,F),Object(d[\"a\"])(n,K.name,K),Object(d[\"a\"])(n,V.name,V),Object(d[\"a\"])(n,z.name,z),Object(d[\"a\"])(n,Z.name,Z),Object(d[\"a\"])(n,X.name,X),Object(d[\"a\"])(n,ee.name,ee),Object(d[\"a\"])(n,ae.name,ae),Object(d[\"a\"])(n,te.name,te),Object(d[\"a\"])(n,ne.name,ne),n),ie=Object.keys(se).map((function(e){return se[e]}));ie=ie.sort((function(e,a){return e.name<a.name?-1:1}));var oe,ce={name:\"Core Set\",encounterSets:[se[\"The Doomsday Chair\"].name,se[\"Masters of Evil\"].name,se[\"Legion of Hydra\"].name,se[\"Bomb Scare\"].name,se.Klaw.name,se.Rhino.name,se.Ultron.name]},ue={name:\"The Rise of Red Skull\",encounterSets:[se[\"Hydra Assault\"].name,se[\"Hydra Patrol\"].name,se[\"Weapon Master\"].name,se[\"Experimental Weapons\"].name,se[\"Under Attack\"].name,se.Crossbones.name,se[\"Absorbing Man\"].name,se.Taskmaster.name,se.Zola.n